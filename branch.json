{
    "conditions": [
        "state": "true, false, both"
    ]
}

count = 0
conditions.forEach { (condition) in
    if condition.state == "true" {
        count += 1
    } else if condition.state == "false" {
        count += 1
    } else {
        count += 2
    }
}

coverage = (count / (conditions.lenght*2))*100